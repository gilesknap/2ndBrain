filters:
  and:
    - file.inFolder("Reference")
    - file.ext == "md"
properties:
  topic:
    displayName: Topic
  tags:
    displayName: Tags
  date:
    displayName: Date
views:
  - type: table
    name: All Reference
    order:
      - file.name
      - topic
      - date
      - tags
    columnSize:
      note.date: 161
